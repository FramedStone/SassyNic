{
  "manifest_version": 3,
  "author": "leewx07@gmail.com",
  "name": "SassyNic",
  "version": "1.0.0",
  "minimum_chrome_version": "131.0.6778.86",
  "description": "Extract class details from CliC, generate possible timetable combinations and auto enrollment.",
  "permissions": ["activeTab", "scripting", "storage", "tabs", "windows"],
  "action": {
    "default_popup": "./popup/popup.html"
  },
  "background": {
    "service_worker": "./scripts/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://clic.mmu.edu.my/*"],
      "js": ["./scripts/content.js"]
    }
  ]
}
